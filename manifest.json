{
  "name": "YouTube Ad Skipper",
  "description": "Auto skip ad on Youtube if exists",
  "manifest_version": 3,
  "version": "0.1",    
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
		"*://*.phimmoizz.net/phim/*/xem-phim.html"
      ],      
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/content.js"
      ]
    }
  ]
}
